{
  "name": "Gamer",
  "plural": "gamers",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "userName": "string",
    "discordUserId": "string",
    "roles": {
      "type": "array",
      "default": [
        "Guest"
      ]
    },
    "discordAvatarURL": "string",
    "lastDiscordActivity": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "games": {
      "type": "hasMany",
      "model": "Game",
      "through": "Gamelog"
    }
  },
  "acls": [],
  "methods": {}
}
